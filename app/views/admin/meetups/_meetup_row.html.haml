%tr
  %td{style: 'width: 20%'}
    = link_to meetup do
      = meetup.name
  %td{style: 'width: 5%'}
    = meetup.rsvps.count
  %td{style: 'width: 20%'}
    = meetup.date.to_formatted_s(:long)
  %td{style: 'width: 20%'}
    = link_to meetup.sponsor.name, meetup.sponsor
  %td{style: 'width: 5%'}
    = link_to 'Edit', edit_admin_meetup_path(meetup), class: 'a-button'
