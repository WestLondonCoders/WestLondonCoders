%tr
  %td{style: 'width: 20%'}
    = link_to event do
      = event.name
  %td{style: 'width: 5%'}
    = event.rsvps.count
  %td{style: 'width: 20%'}
    = event.date.to_formatted_s(:long)
  %td{style: 'width: 20%'}
    = link_to event.sponsor.name, event.sponsor
  %td{style: 'width: 5%'}
    = link_to 'Edit', edit_admin_event_path(event), class: 'a-button'
