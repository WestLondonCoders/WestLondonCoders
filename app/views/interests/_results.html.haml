.a-tags
  - @interests.each do |interest|
    .a-tag--with-face
      - if interest.users && interest.users.first
        .m-faces--team__face
          = link_to image_tag(interest.users.first.image), user_path(interest.users.first), title: interest.users.first.name
      %h2.a-tag.a-tag--interest
        = interest.name
